{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AlleloPNM",
  "version": "0.1.0",
  "identifier": "eco.allelo.pnm.prototype",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "PNM Prototype",
        "width": 1280,
        "height": 960,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "iOS" :{
      "developmentTeam": "V2YR9RLTN7"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
